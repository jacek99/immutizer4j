<FindBugsFilter>

    <!-- Skip JUnit test code -->
    <Match>
        <Package name="~test\..*"/>
    </Match>
    <Match>
        <Class name="~.*Test$"/>
    </Match>

    <!-- False positives -->
    <Match>
        <Class name="org.immutizer4j.test.sample.ParentPojo"/>
        <Bug code="SS"/>
    </Match>

</FindBugsFilter>
